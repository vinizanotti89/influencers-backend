<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <suppress>
        <notes>Falso positivo para React Dev Dependencies</notes>
        <packageUrl regex="true">^pkg:npm/react@.*$</packageUrl>
        <vulnerabilityName>Desatualização de Dependência</vulnerabilityName>
    </suppress>
    
    <suppress>
        <notes>Supressão para dependências de desenvolvimento</notes>
        <packageUrl regex="true">^pkg:npm/webpack@.*$</packageUrl>
        <cve>CVE-2021-0001</cve>
    </suppress>


</suppressions>